<?xml version="1.0" encoding="utf-8"?>


<!-- NOTE: The id of the topmost container below must match the XML filename -->

<cmu.xprize.robotutor.tutorengine.widgets.core.TScenePercentLayout

    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/arithmetic"
    android:background="#F3DB">

    <cmu.xprize.robotutor.tutorengine.widgets.core.TBanner
        android:id="@+id/Sbanner"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_heightPercent="10%"
        android:background="#800"
        android:gravity="center" />

    <cmu.xprize.robotutor.tutorengine.widgets.core.TAsmComponent
        android:id="@+id/SAddSubtract"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_heightPercent="80%"
        app:layout_marginTopPercent="5%"
        app:layout_marginRightPercent="0%"
        app:layout_marginLeftPercent="5%"
        app:radAlley="0.33"
        android:background="#0000"/>

</cmu.xprize.robotutor.tutorengine.widgets.core.TScenePercentLayout>




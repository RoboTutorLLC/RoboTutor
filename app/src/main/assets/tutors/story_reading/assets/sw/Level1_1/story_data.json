  "story_name": "Ajali mbaya",
  "data": [
    {
      "image": "Level1_1_Page_02_Image_0001.jpg",
      "text": [
        [
          "Siku moja, mimi na dada yangu tulikuwa tukitembea karibu na barabara."
        ]
      ]
    },
    {
      "image": "Level1_1_Page_03_Image_0001.jpg",
      "text": [
        [
          "Lori kubwa ilikuwa ikiendeshwa kuteremka mlima."
        ]
      ]
    },
    {
      "image": "Level1_1_Page_04_Image_0001.jpg",
      "text": [
        [
          "Gari ndogo lilikuwa likielekea upande wa juu wa barabara kwa kasi."
        ]
      ]
    },
    {
      "image": "Level1_1_Page_05_Image_0001.jpg",
      "text": [
        [
          "Dereva wa lori alikuwa akizungumza kwa simu ya rununu."
        ],
        [
          "Hakuwa akizingatia barabara."
        ]
      ]
    },
    {
      "image": "Level1_1_Page_06_Image_0001.jpg",
      "text": [
        [
          "Lori liligonga gari!"
        ]
      ]
    },
    {
      "image": "Level1_1_Page_07_Image_0001.jpg",
      "text": [
        [
          "Gari la hospitali pamoja na polisi walifika kutoa usaidizi."
        ]
      ]
    },
    {
      "image": "Level1_1_Page_08_Image_0001.jpg",
      "text": [
        [
          "Magari mawili ya kuvuta magari yalifika."
        ],
        [
          "Moja lilivuta lori."
        ],
        [
          "Lingine likavuta gari."
        ]
      ]
    },
    {
      "image": "Level1_1_Page_09_Image_0001.jpg",
      "text": [
        [
          "Tuliona kila kitu."
        ],
        [
          "Tulitishika sana."
        ]
      ]
    }
  ],
  "level": "1",
  "titleimage": "Level1_1_Page_01_Image_0001.jpg",
  "language": "Kiswahili",
